package while2;

import java.util.Scanner;

public class exercicio4 {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int populaA = 80000;
		int populaB = 200000;
		int cont = 0;
		
		while (populaA <= populaB) {
			populaA += (populaA/100) * 3;
			populaB += (populaB/100) * 1.5;
			cont++;
			
		}
		
	}

}
