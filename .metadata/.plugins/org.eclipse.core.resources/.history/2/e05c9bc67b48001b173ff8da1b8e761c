package while2;

import java.util.Scanner;

public class exercicio20_MediaIdade {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		
		int n = scan.nextInt();
		int idade = 0;
		int soma= 0;
		for(int i=1;i<n ;i++) {
			System.out.println("Qual a sua idade?");
			idade = scan.nextInt();
			soma += idade;
		}
	}

}
